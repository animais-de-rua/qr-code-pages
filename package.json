{
  "name": "adr-qr-code",
  "version": "1.0.0",
  "description": "JavaScript example starter project",
  "main": "index.html",
  "scripts": {
    "start": "npm run clear-cache && parcel index.html --open",
    "build": "npm run clear-cache && parcel build index.html --public-url /qr --no-source-maps",
    "clear-cache": "del-cli --force ./.cache && del-cli --force ./dist"
  },
  "dependencies": {
    "bootstrap": "5.0.2",
    "cantil": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "del-cli": "^3.0.1",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "parcel-bundler": "^1.12.4",
    "posthtml": "^0.15.1",
    "posthtml-expressions": "^1.5.0",
    "posthtml-include": "^1.5.0",
    "sass": "^1.35.1"
  },
  "resolutions": {},
  "keywords": [
    "javascript",
    "starter"
  ]
}
